{
  "name": "quasar-dapp",
  "version": "1.0.0",
  "description": "Quasar v0.15 DApp boilerplate with web3 provider either via browser injection or infura on mobile devices",
  "productName": "Quasar DApp",
  "cordovaId": "org.cordova.quasar.app",
  "author": "Ryan Hendricks <RyanHendricks@gmail.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"No test specified\" && exit 0",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test",
    "netlify": "npm install && quasar dev -m pwa"
  },
  "dependencies": {
    "http-server": "^0.11.1",
    "web3": "1.0.0-beta.34",
    "web3-provider-engine": "^13.8.0"
  },
  "devDependencies": {
    "web3": "1.0.0-beta.34",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-vue": "^4.5.0",
    "prettier-eslint": "^8.8.1",
    "quasar-cli": "^0.15.18"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "main": "https://ryanhendricks.github.io/quasar-dapp/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RyanHendricks/quasar-dapp.git"
